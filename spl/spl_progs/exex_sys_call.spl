    while([INODE_TABLE +[PROCESS_TABLE+([SYSTEM_STATUS_TABLE+1])*16 +7]*16+i]!=-1) do
         multipush(R0,R1,R2,R8,R9,R10);
        R1=1;

        call MOD_2;
        [pagetnew+8+i-8]=R0;
        [pagetnew+9+i-8]="0100";
